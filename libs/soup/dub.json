{
	"dependencies": {
		"glibd": "~>2.3.0"
	},
	"name": "soup",
	"preGenerateCommands-posix": [
		"dub fetch girtod || true",
		"cd $PACKAGE_DIR && dub run girtod -- --use-runtime-linker"
	],
	"preGenerateCommands-windows": [
		"dub fetch girtod || VER>NUL",
		"cd $PACKAGE_DIR && dub run girtod -- --use-runtime-linker"
	],
	"sourcePaths": [
		"out"
	],
	"importPaths": ["out", "source"],
	"targetType": "library"
}
